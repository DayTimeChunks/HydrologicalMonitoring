@article{Gao2004,
abstract = {Although both raindrop driven processes and diffusion play important roles in the transfer of chemicals from soil to surface runoff, current transport models either do not consider the two processes together, or use `effective' parameters with uncertain physical definitions. We developed a physically based, solute transport model that couples both mechanisms and tested it with laboratory experiments. One unique aspect of this study is that all the parameters were either directly measured or previously published, that is, there was no model `calibration' or `fitting.' Our model assumes that chemicals near the surface of the soil are ejected into runoff by raindrop impact and chemicals deeper in the soil diffuse into a surface layer, or `exchange layer,' via diffusion. The exchange layer depth and transfer processes are derived from the `shield' concept in the Rose soil erosion model (e.g. Rose, 1985). The model's governing equations were solved numerically and the results agreed well with experimental data (R2{\textgreater}0.90). The model's sensitivity to various physical and chemical parameters illuminated the importance of both raindrop controlled processes and diffusion on chemical transport from soil to surface runoff.},
author = {Gao, Bin and Todd and Steenhuis, Tammo and Hogarth, William and Parlange, Yves},
doi = {10.1016/j.jhydrol.2004.03.026},
file = {:C$\backslash$:/Users/pablo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gao et al. - 2004 - Rainfall induced chemical transport from soil to runoff theory and experiments.pdf:pdf},
issn = {00221694},
journal = {Journal of Hydrology},
keywords = {chemical transport,rainfall},
number = {1-4},
pages = {291--304},
title = {{Rainfall induced chemical transport from soil to runoff: theory and experiments}},
url = {http://dx.doi.org/10.1016/j.jhydrol.2004.03.026},
volume = {295},
year = {2004}
}
