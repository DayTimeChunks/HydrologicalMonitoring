file = {:C$\backslash$:/Users/pablo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bashir et al. - 2013 - Enantioselective carbon stable isotope fractionation of hexachlorocyclohexane during aerobic biodegradation by Sp.pdf:pdf},
