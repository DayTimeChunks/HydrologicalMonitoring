file = {:C$\backslash$:/Users/pablo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sch{\"{a}}fer, Sch{\"{a}}fer, Kinzelbach - 1998 - Simulation of reactive processes related to biodegradation in aquifers. 2. Model application to a.pdf:pdf},
isbn = {0169-7722},
issn = {01697722},
journal = {Journal of Contaminant Hydrology},
keywords = {Biodegradation,Column study,Modelling,Reactive transport},
number = {1-2},
pages = {187--209},
title = {{Simulation of reactive processes related to biodegradation in aquifers. 2. Model application to a column study on organic carbon degradation}},
volume = {31},
year = {1998}
