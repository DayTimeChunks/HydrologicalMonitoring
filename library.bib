journal = {Ecological Engineering},
keywords = {Biological growth,Bromide tracer,Flow patterns,Horizontal subsurface flow constructed wetlands,Hydraulic parameters},
number = {2},
pages = {124--133},
title = {{Change in flow and transport patterns in horizontal subsurface flow constructed wetlands as a result of biological growth}},
volume = {27},
year = {2006}
}
