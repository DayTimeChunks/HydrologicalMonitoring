isbn = {0048-9697},
issn = {18791026},
journal = {Science of the Total Environment},
keywords = {Advanced treatment,Fate,Micropollutants,Occurrence,Removal,WWTP},
pages = {619--641},
pmid = {24394371},
publisher = {Elsevier B.V.},
title = {{A review on the occurrence of micropollutants in the aquatic environment and their fate and removal during wastewater treatment}},
url = {http://dx.doi.org/10.1016/j.scitotenv.2013.12.065},
