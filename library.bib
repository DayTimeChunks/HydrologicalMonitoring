keywords = {Biogeochemical reactions,Kinetic reactions,Operator splitting,Porous media,Reactive transport modeling},
month = {mar},
number = {3},
pages = {397--405},
title = {{GeoSysBRNS—A flexible multidimensional reactive transport model for simulating biogeochemical subsurface processes}},
url = {http://www.sciencedirect.com/science/article/pii/S0098300409002957},
volume = {36},
year = {2010}
}
