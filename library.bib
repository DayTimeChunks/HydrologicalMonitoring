doi = {10.1016/j.chemosphere.2009.11.033},
file = {:C$\backslash$:/Users/pablo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Schmidt et al. - 2010 - Aerobic biodegradation of cis-1,2-dichloroethene as sole carbon source Stable carbon isotope fractionation and g.pdf:pdf},
issn = {00456535},
journal = {Chemosphere},
keywords = {Aerobic mineralization,Bioremediation,Cis-1,2-dichloroethene,Growth yield,Natural attenuation,Stable carbon enrichment factor},
number = {5},
pages = {527--532},
