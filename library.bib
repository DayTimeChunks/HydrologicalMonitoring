doi = {10.1080/17538947.2013.769783},
file = {:C$\backslash$:/Users/pablo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Yang, Xu, Nebert - 2013 - Redefining the possibility of digital Earth and geosciences with spatial cloud computing.pdf:pdf},
isbn = {1753-8947},
issn = {1753-8947},
journal = {International Journal of Digital Earth},
keywords = {1,and digital earth,cloudgis,earth science,earthcube,environmental sciences,escience,geochemistry,hydrology,including geography,interoperability,previously,require the integration of,separate fields,technology advancements and globalization,the challenges of geosciences},
number = {November 2014},
pages = {297--312},
title = {{Redefining the possibility of digital Earth and geosciences with spatial cloud computing}},
url = {http://www.tandfonline.com/doi/abs/10.1080/17538947.2013.769783{\%}5Cnhttp://biblioproxy.cnr.it:2267/doi/full/10.1080/17538947.2013.769783{\%}5Cnhttp://biblioproxy.cnr.it:2267/doi/pdf/10.1080/17538947.2013.769783},
volume = {6},
