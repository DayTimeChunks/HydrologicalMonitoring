@article{Fuchs2011,
abstract = {Life cycle assessment (LCA) is used to compare the environmental impacts of vertical flow constructed wetlands (VFCW) and horizontal flow constructed wetlands (HFCW). The LCAs include greenhouse gas (N
                        2O, CO
