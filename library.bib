journal = {Journal of Hydrology},
keywords = {Infiltration,Macropore flow,Overland flow,Simulation and interaction,Vadose zone},
number = {1-4},
pages = {116--127},
title = {{A model to couple overland flow and infiltration into macroporous vadose zone}},
url = {http://dx.doi.org/10.1016/S0022-1694(98)00179-6},
volume = {210},
year = {1998}
}
