doi = {10.1021/es103728q},
file = {:C$\backslash$:/Users/pablo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fletcher et al. - 2011 - Stable carbon isotope enrichment factors for cis-1,2-dichloroethene and vinyl chloride reductive dechlorination.pdf:pdf},
