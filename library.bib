issn = {00456535},
journal = {Chemosphere},
keywords = {Chiral pharmaceuticals,Enantiomeric fraction,Enantiomers,Removal efficiency,Wastewater treatment},
number = {2},
pages = {200--205},
pmid = {19155040},
publisher = {Elsevier Ltd},
title = {{Assessment of the pharmaceutical active compounds removal in wastewater treatment systems at enantiomeric level. Ibuprofen and naproxen}},
