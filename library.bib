journal = {Talanta},
keywords = {EDTA,Heavy metals,Kinetic extraction models,Metal fractionation},
number = {4},
pages = {844--857},
pmid = {18970062},
title = {{Kinetic approach to heavy metal mobilization assessment in sediments: Choose of kinetic equations and models to achieve maximum information}},
volume = {66},
year = {2005}
