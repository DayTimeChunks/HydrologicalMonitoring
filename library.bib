@article{Rodgers2004,
abstract = {This work addressed the seasonal and successional factors of root$\backslash$ndynamics in natural and restoration Atlantic white cedar (AWC) wetlands.$\backslash$nUsing minirhizotrons and soil root cores, fine root dynamics were$\backslash$nmeasured in a chronosequence of reference and restoration AWC wetlands$\backslash$nto compare trends in ecosystem development after canopy harvest.$\backslash$nSeasonal fine root abundance, production, and mortality were sampled$\backslash$nduring a 439-day period in one restoration and three reference AWC$\backslash$nwetlands. Soil cores were collected to measure fine root biomass$\backslash$nand to determine allometric relationships between root length and$\backslash$nbiomass. Significant seasonal variation of root dynamics was observed$\backslash$nin the young reference and restoration sites. The mature and intermediate-aged$\backslash$nsites exhibited little seasonal variability in root abundance and$\backslash$nmortality. Root production was variable but not seasonally consistent.$\backslash$nResults suggest that root dynamics become less seasonal as AWC communities$\backslash$nshift from herbaceous to woody vegetation dominance. No trend in$\backslash$nfine root abundance along the chronosequence was observed, suggesting$\backslash$nthat roots rapidly reestablish following tree harvest. Measurements$\backslash$nof annual root length production suggest increasing annual production$\backslash$nwith decreasing stand age. However, a reversal of this trend was$\backslash$nobserved when using production estimates calculated from minirhizotron$\backslash$nmeasurements and root length-mass relationships. These findings underscore$\backslash$nthe importance of supplementing minirhizotron data with root allometric$\backslash$nrelationships when analyzing vegetation gradients. Overall, results$\backslash$nindicate substantial differences in the form and quantity of root$\backslash$ncontributions to soil organic matter in the restoration site compared$\backslash$nto that in the reference chronosequence. Higher initial planting$\backslash$ndensities of AWC are recommended to achieve similar contributions$\backslash$nof roots to soil organic matter accumulation in the restoration site.},
author = {Rodgers, H. LeRoy and Day, Frank P. and Atkinson, Robert},
doi = {10.1111/j.1061-2971.2004.00211.x},
file = {:C$\backslash$:/Users/pablo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rodgers, Day, Atkinson - 2004 - Root dynamics in restored and naturally regenerated Atlantic white cedar wetlands.pdf:pdf},
issn = {10612971},
journal = {Restoration Ecology},
keywords = {Atlantic white clear,Chronosequence,Fine roots,Forested wetlands,Minirhizotrons,Restoration,Root length density,Root production},
number = {3},
pages = {401--411},
title = {{Root dynamics in restored and naturally regenerated Atlantic white cedar wetlands}},
volume = {12},
year = {2004}
}
@article{Mcgrath2010,
abstract = {How can leaching risk be assessed if the chemical flux and/or the toxicity is highly uncertain? For many strongly sorbing pesticides it is known that their transport through the unsaturated zone occurs intermittently through preferential flow, triggered by significant rainfall events. In these circumstances the timing and frequency of these rainfall events may allow quantification of leaching risk to overcome the limitations of flux prediction. In this paper we analyze the leaching behavior of bromide and two herbicides, methabenzthiazuron and ethidimuron, using data from twelve uncropped lysimeters, with high-resolution climate data, in order to identify the rainfall controls on rapid solute leaching. A regression tree analysis suggested that a coarse-scale fortnightly to monthly water balance was a good predictor of short-term increases in drainage and bromide transport. Significant short-term herbicide leaching, however, was better predicted by the occurrence of a single storm with a depth greater than a 19 mm threshold. Sampling periods where rain events exceeded this threshold accounted for between 38{\%} and 56{\%} of the total mass of herbicides leached during the experiment. The same threshold only accounted for between 1{\%} and 10{\%} of the total mass of bromide leached. On the basis of these results, we conclude that in this system, the leaching risks of strongly sorbing chemicals can be quantified by the timing and frequency of these large rainfall events. Empirical and modeling approaches are suggested to apply this frequentist approach to leaching risk assessment to other soil-climate systems.},
author = {Mcgrath, G S and Hinz, C and Sivapalan, M and Dressel, J and Pu, T},
doi = {10.1029/2008WR007506},
file = {:C$\backslash$:/Users/pablo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mcgrath et al. - 2010 - Identifying a rainfall event threshold triggering herbicide leaching by preferential flow.pdf:pdf},
journal = {Water Resources Research},
keywords = {climate,doi:10.1029/2008WR007506,frequency,http://dx.doi.org/10.1029/2008WR007506,lysimeter,pesticide,preferential flow,rainfall,threshold},
pages = {1--12},
title = {{Identifying a rainfall event threshold triggering herbicide leaching by preferential flow}},
volume = {46},
year = {2010}
}
