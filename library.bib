isbn = {0009-2541},
issn = {00092541},
journal = {Chemical Geology},
keywords = {Fe(III) oxyhydroxide solubility,Michaelis-Menten kinetics,Microbial iron reduction},
number = {3-4 SPEC.ISS.},
pages = {255--268},
pmid = {225611600006},
title = {{Microbial reduction of iron(III) oxyhydroxides: Effects of mineral solubility and availability}},
volume = {212},
year = {2004}
}
