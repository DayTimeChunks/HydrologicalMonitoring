file = {:C$\backslash$:/Users/pablo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kahl, Sidorenko, Gottesbren - 2015 - Local and global inverse modelling strategies to estimate parameters for pesticide leaching from ly.pdf:pdf},
issn = {15264998},
journal = {Pest Management Science},
keywords = {Inverse modelling,Levenberg-Marquardt,Lysimeter,Optimisation,Parameter estimation,Pesticide,SCEM},
number = {4},
pages = {616--631},
pmid = {25252181},
title = {{Local and global inverse modelling strategies to estimate parameters for pesticide leaching from lysimeter studies}},
